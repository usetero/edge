{
  "policies": [
    {
      "id": "drop-0000",
      "name": "Drop policy 0",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "authentication"
          }
        ],
        "keep": "none"
      }
    },
    {
      "id": "drop-0001",
      "name": "Drop policy 1",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "NOMATCH_BENCH_0001_4f403999"
          }
        ],
        "keep": "none"
      }
    },
    {
      "id": "keep-0002",
      "name": "Keep policy 2",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "payment"
          }
        ],
        "keep": "all",
        "transform": {
          "redact": [
            {
              "log_attribute": "credit_card",
              "replacement": "[REDACTED]"
            },
            {
              "log_attribute": "ssn",
              "replacement": "[REDACTED]"
            }
          ]
        }
      }
    },
    {
      "id": "keep-0003",
      "name": "Keep policy 3",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "authentication|login"
          }
        ],
        "keep": "all",
        "transform": {
          "add": [
            {
              "log_attribute": "security_category",
              "value": "auth",
              "upsert": true
            }
          ]
        }
      }
    },
    {
      "id": "sample-0004",
      "name": "Sample policy 4",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "Cache"
          }
        ],
        "keep": "10%"
      }
    },
    {
      "id": "rate-0005",
      "name": "Rate limit policy 5",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "info|INFO"
          }
        ],
        "keep": "100/s"
      }
    },
    {
      "id": "rate-0006",
      "name": "Rate limit policy 6",
      "log": {
        "match": [
          {
            "log_field": "body",
            "regex": "NOMATCH_BENCH_0006_834bdb90"
          }
        ],
        "keep": "500/s"
      }
    },
    {
      "id": "metric-drop-0000",
      "name": "Metric drop policy 0",
      "metric": {
        "match": [
          {
            "metric_field": "name",
            "regex": "payment"
          }
        ],
        "keep": false
      }
    },
    {
      "id": "metric-keep-0001",
      "name": "Metric keep policy 1",
      "metric": {
        "match": [
          {
            "metric_field": "name",
            "regex": "NOMATCH_BENCH_0001_4a35caf6"
          }
        ],
        "keep": true
      }
    },
    {
      "id": "metric-keep-0002",
      "name": "Metric keep policy 2",
      "metric": {
        "match": [
          {
            "metric_field": "name",
            "regex": "NOMATCH_BENCH_0002_b7ebb413"
          }
        ],
        "keep": true
      }
    }
  ]
}
